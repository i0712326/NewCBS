@CHARSET "UTF-8";
body{
	font: 62.5%;
	margin: 5px;
}

.ui-dialog{
	box-shadow: 0 0 0 4px #333333;
}
.textbox {
	background-image: url(/css/img/form_bg.jpg);
	background-repeat: repeat-x;
	border: 1px solid #d1c7ac;
	width: 85px;
	color: #333333;
	padding: 3px;
	margin-right: 4px;
	margin-bottom: 8px;
	font-family: tahoma, arial, sans-serif;
}
#header {
	width:100%;
	height:30px;
	background:#8cc4f8 url(images/ui-bg_inset-soft_85_8cc4f8_1x100.png) 50% 50% repeat-x;
	border:1px solid #068cf4;
	text-align:right;
	
}

#nav {
	width:200px;
	float:left;
	text-align:left;
}

#MENU {
	height:490px;
}

#ADMIN{
	height:490px;
}

#SETTING{
	height:490px;
}

#icons {
	margin: 0;
	padding: 0;
	display:inline-block;
}

#icons li {
	margin: 2px;
	position: relative;
	padding: 4px 0;
	cursor: pointer;
	float: left;
	list-style: none;
}

#icons span.ui-icon {
	float: left;
	margin: 0 4px;
}

#FUNCNAME {
	width:120px;
	vertical-align:middle;
	margin-bottom:27px;
}

input {
	text-align : right;
}
