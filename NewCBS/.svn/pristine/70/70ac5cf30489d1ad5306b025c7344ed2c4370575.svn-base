<?xml version="1.0" encoding="UTF-8" ?>
<%@ page language="java" contentType="text/xml; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%@ taglib prefix="s" uri="/struts-tags" %>
<data>
	<response>
		<s:property value="response"/>
	</response>
	<message>
		<s:property value="message"/>
	</message>
	<state>
		<number>
			<s:text name="format.number">
				<s:param name="value" value="stateNum"/>
			</s:text>
		</number>
		<amount>
			<s:text name="format.money">
				<s:param name="value" value="stateTotal"/>
			</s:text>
		</amount>
		<fee>
			<s:text name="format.money">
				<s:param name="value" value="stateFee"/>
			</s:text>
		</fee>
		<net>
			<s:text name="format.money">
				<s:param name="value" value="stateNet"/>
			</s:text>
		</net>
	</state>
	<settle>
		<number>
			<s:text name="format.number">
				<s:param name="value" value="settleNum"/>
			</s:text>
		</number>
		<amount>
			<s:text name="format.money">
				<s:param name="value" value="settleTotal"/>
			</s:text>
		</amount>
		<fee>
			<s:text name="format.money">
				<s:param name="value" value="settleFee"/>
			</s:text>
		</fee>
		<net>
			<s:text name="format.money">
				<s:param name="value" value="settleNet"/>
			</s:text>
		</net>
	</settle>
</data>